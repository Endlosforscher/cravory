{
  "name": "cravory",
  "private": true,
  "description": "Cravory is a recipes finder built with React and Vite by Luigi Sabbetti",
  "version": "0.0.1",
  "type": "module",
  "author": "Luigi Sabbetti",
  "license": "MIT",
  "keywords": [
    "recipes",
    "finder",
    "react",
    "vite"
  ],
  "repository": {
    "type": "git",
